{
    "fileExtensions": [ ".less", ".css" ],
    "excludedFiles": [ "./node_modules/**/*.less"],
    "spaceAfterPropertyColon": {
        "enabled": true,
        "style": "one_space"
    },
    "spaceAroundComma": {
        "style": "after",
        "allowNewline": true
    },
    "importPath": {
        "filenameExtension": true
    },
    "newlineAfterBlock": true,
    "finalNewline": false,
    "emptyRule": true,
    "singleLinePerSelector": true
}